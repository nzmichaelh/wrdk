  22              	;==============================================
  23              	;＜レジスタ説明＞
  24              	;	%r2:指数部２
  25              	;	%r4:符号ビット１[0(+) or 1(-)]
  26              	;	%r5:符号ビット２[0(+) or 1(-)]/指数部１/mask
  27              	;	%r6:引数１(float)/仮数部１
  28              	;	%r7:引数２(float)/仮数部２
  29              	;==============================================
  30              	
  31              	
  32              	__fcmps:
  33 0000 0202     		pushn	%r2				; %r0〜%r2 待避
  34              	
  35 0002 642E     		ld.w	%r4, %r6		; 符号ビット１(%r4) <- 引数１(%r6)
  36 0004 149C     		rl		%r4, 1			; rotate left 1 bit
  37 0006 1470     		and		%r4, 1			; keep LSB
  38              	
  39 0008 752E     		ld.w	%r5, %r7		; 符号ビット２(%r5) <- 引数２(%r7)
  40 000a 159C     		rl		%r5, 1			; rotate left 1 bit
  41 000c 1570     		and		%r5, 1			; keep LSB
  42              	
  43              		; if 符号ビット１(%r4) = 1 and 符号ビット２(%r5) = 0 then CVZN = 1001 (lt)
  44              		; if 符号ビット１(%r4) = 0 and 符号ビット２(%r5) = 1 then CVZN = 0000 (gt)
  45 000e 452A     		cmp		%r5, %r4		; if 符号ビット２(%r5) != 符号ビット１(%r4) 
  46 0010 1F1A     		jrne	end				; then goto end
  47              	
  48              		; case: 符号ビット１(%r4) = 符号ビット２(%r5) 
  49 0012 652E     		ld.w	%r5, %r6		; 指数部１(%r5) <- 引数１(%r6)
  50 0014 158C     		sll		%r5, 1			; 指数部１(%r5) << 1
  51 0016 85888588 		xsrl 	%r5, 24			; 指数部１(%r5) >> 24
  51      8588
  52              	
  53 001c 722E     		ld.w	%r2, %r7		; 指数部２(%r2) <- 引数２(%r7)
  54 001e 128C     		sll		%r2, 1			; 指数部２(%r2) << 1
  55 0020 82888288 		xsrl 	%r2, 24			; 指数部２(%r2) >> 24
  55      8288
