  56              	
  57 0026 1468     		cmp		%r4, 1			; if 符号ビット１(%r4) = 1 
  58 0028 0418     		jreq	negexp			; then goto negexp
  59              	
  60              		; max. 指数部 = 0xff
  61              		; min. 指数部 = 0x00
  62 002a 252A     		cmp		%r5, %r2		; if 指数部１(%r5) != 指数部２(%r2) 
  63 002c 111A     		jrne	end				; then goto end
  64 002e 031E     		jp		mancmp
  65              	
  66              	negexp:
  67 0030 522A     		cmp		%r2, %r5		; if 指数部２(%r2) !=  指数部１(%r5)
  68 0032 0E1A     		jrne	end				; then goto end
  69              	
  70              	mancmp:
  71              		; check if Inf or NaN values
  72 0034 03C0F56B 		xcmp	%r5, 0xff		; if 指数部１(%r5) = 指数部２(%r2) = 特殊数(0xff) 
  73 0038 0B18     		jreq	end				; then goto end
  74              	
  75              		; case: 指数部１(%r5) = 指数部２(%r2)
  76 003a 0FC0FFDF 		xld.w	%r5, 0x7fffff	; mask(%r5) <- 0x7fffff
  76      F56F
  77              	
  78 0040 5632     		and		%r6, %r5		; 仮数部１(%r6) = 仮数部１(%r6) & mask(%r5)
  79 0042 5732     		and		%r7, %r5		; 仮数部２(%r7) = 仮数部２(%r7) & mask(%r5)
  80              	
  81 0044 1468     		cmp		%r4, 1			; if 符号ビット１(%r4) = 1 
  82 0046 0318     		jreq	negman			; then goto negman
  83              	
  84 0048 762A     		cmp		%r6, %r7		; compare 仮数部１(%r6) : 仮数部２(%r7) 
  85 004a 021E     		jp		end
  86              	
  87              	negman:
  88 004c 672A     		cmp		%r7, %r6		; compare 仮数部２(%r7) : 仮数部１(%r6) 
